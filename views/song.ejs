<% include partials/header %>
   <a class="btn btn-info" href="/songs">
    <span class="glyphicon glyphicon-arrow-left" aria-hidden="true"></span> All songs
   </a>
   <h1><%=song.title%></h1>
   <hr/>
   Album  : <%=song.album%><br/>
   Artist : <%=song.artist%><br/>
   <% if (song.year) { %>
      Year   : <%=song.year%><br/>
   <% } %>
   <% if (song.bpm) { %>
      BPM   : <%=song.bpm%><br/>
   <% } %>
   <br>
   <a class="btn btn-danger" href="/songs/edit/<%=song.id%>">
    <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Edit
   </a>

<% include partials/footer %>